{
  "name": "svg-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "server": "bun run server/index.ts",
    "dev:server": "bun --watch server/index.ts",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:security": "curl -s http://localhost:3001/security/test | jq",
    "test:openai": "bun run scripts/test-openai.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "build:prod": "./scripts/build-prod.sh",
    "deploy": "./scripts/deploy.sh",
    "monitor": "./scripts/monitor.sh",
    "docker:build": "docker build -t svg-ai:latest .",
    "docker:run": "docker run -d --name svg-ai-prod -p 3001:3001 --env-file .env.production svg-ai:latest",
    "docker:compose": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:logs": "docker logs -f svg-ai-prod",
    "docker:stop": "docker stop svg-ai-prod && docker rm svg-ai-prod",
    "configure": "./scripts/configure-production.sh",
    "configure:cloudflare": "./scripts/configure-cloudflare.sh",
    "deploy:cloudflare": "./scripts/deploy-cloudflare.sh",
    "setup:cname": "./scripts/setup-cname-domain.sh",
    "deploy:pages": "./scripts/deploy-pages-simple.sh",
    "deploy:functions": "./scripts/deploy-with-functions.sh",
    "deploy:simple": "./scripts/deploy-simple-pages.sh",
    "deploy:clean": "./scripts/deploy-clean.sh",
    "deploy:enhanced": "./scripts/deploy-enhanced.sh",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "bun run server/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun run server/db/seed.ts",
    "db:reset": "bun run server/db/reset.ts",
    "db:setup": "./scripts/setup-database.sh"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.20",
    "bun-types": "^1.2.21",
    "drizzle-kit": "^0.31.4",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "terser": "^5.44.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.29"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.0",
    "@types/pg": "^8.15.5",
    "@types/ws": "^8.18.1",
    "drizzle-orm": "^0.44.5",
    "hono": "^4.9.6",
    "isomorphic-dompurify": "^2.26.0",
    "openai": "^5.20.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "vue": "^3.4.37",
    "ws": "^8.18.3",
    "zod": "^4.1.5",
    "encore.dev": "^1.49.3"
  }
}